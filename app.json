{
  "expo": {
    "name": "Gringo Delivery",
    "slug": "gringo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/favicon_trim.png",
    "scheme": "gringo-gelivery",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "package": "com.gringodelivery.gringodelivery",
    "versionCode": 1,
    "ios": {
      "config": {
        "googleMapsApiKey": "AIzaSyBh-mZCvYaGg_LrtNbb1Ho8e5AZWIauDn4"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.gringodelivery.gringodelivery",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/gringo-logo-colorida.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBh-mZCvYaGg_LrtNbb1Ho8e5AZWIauDn4"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.gringodelivery.gringodelivery"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon_trim.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Gringo to use your location to track your deliveries."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/gringo-logo-colorida.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9b6030dd-51df-498b-9d63-d0de6882b51a"
      }
    }
  }
}
